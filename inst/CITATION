desc <- packageDescription("gmvalid")
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", desc$Date)
vers <- paste("R package version", desc$Version)
citEntry(entry="Manual",
title = "gmvalid: Validation of Graphical Models",
author = personList(as.person("Ronja Foraita"),
as.person("Fabian Sobotka"),
year = year,
note = vers,
textVersion =
paste("Ronja Foraita, Fabian Sobotka (",
year,
"). gmvalid: Validation of Graphical Models. ",
vers, ".", sep="")))
